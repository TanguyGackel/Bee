             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BeeClient.Head">
    
    <DockPanel>
        <Border Background="Black" DockPanel.Dock="Top" Padding="10">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="KillerBee" FontSize="20" VerticalAlignment="Top" Margin="10,10,50,5"/>
                <TabControl>
                    <TabItem Header="Freezbee"/>
                    <TabItem Header="Test"/>
                    <TabItem Header="Ingrédient"/>
